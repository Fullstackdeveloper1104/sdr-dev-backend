<script>

  $(document).ready(function() {
    document.ookie = "<%callOptions[0].LeadId%>"
    // Initialize DataTable with initial options
    var table = $('#example').DataTable({
      searching: false,
      destroy: true,
      paging: false,
      info: false
    });

    // Handle row click event
    $('#example tbody').on('click', 'tr', function () {
      var rowData = table.row(this).data();
      console.log(rowData); // Get data of the clicked row
      // Now, you can populate elements on the page with data from the clicked row
      $('#lead-id').text(rowData[0]);
      $('#contact-name').val(rowData[1]);
        $('#company-name').val(rowData[2]);
        $('#title').val(rowData[3]);
        $('#industry').val(rowData[4]);
        $('#timezone').val(rowData[5]);
        $('#address').val(rowData[6]);
        $('#city').val(rowData[7]);
        $('#state').val(rowData[8]);
        $('#website').attr('href',rowData[9]);
        $('#email').attr('href','mailto:'+rowData[10]);
        $('#call-category').text(rowData[11]);
        $('#campaign-type').text(rowData[12]);
        $('#call-notes-area').text(rowData[13]);
        $('#call-date').text(rowData[14]);

      //   set cookie leadId
        document.cookie = "LeadId=" + rowData[0];



      table.clear().draw();
      // Close the modal
      $('#close-search-modal').click()

      // reload
        location.reload();
    });

    // Initial setup based on the default value of #input-queryType
    updateInputField();

    // Handle changes in the #input-queryType dropdown
    $("#input-queryType").change(function() {
      updateInputField();
    });

    // Handle key-up event on #tags
    $('#tags').keyup(function () {
      // Show the table header
      $('#example thead').css('visibility', 'visible');
      var search = $('#tags').val();
      var queryType = $("#input-queryType").val();

      // Perform AJAX search
      $.ajax({
        url: '/call-options-search',
        method: 'post',
        data: {
          search: search,
          queryType: queryType
        },
        success: function (response) {
          console.log(response);

          // Clear existing rows in the DataTable
          table.clear().draw();

          // Populate the table with the response data
          response.data.forEach(function (item) {
            console.log(item);
            var dateString = item.CallDate;
            var convertedDate = formatDate(dateString);

            console.log(item);
            table.row.add([
              item.LeadId,
              item.ContactName,
              item.CompanyName,
              item.Title,
                item.Industry,
              item.TimeZoneName,
              item.Address_1,
              item.City,
              item.State,
                item.Website,
                item.Email,
                item.CallCategory,
                item.CampaignType,
                item.CallerNotes,
                convertedDate




              // Add more columns as needed
            ]).draw();
          });
        }
      });
    });
  });

  function formatDate(dateString) {
    var options = { year: 'numeric', month: 'long', day: 'numeric' };
    var date = new Date(dateString);
    return date.toLocaleDateString('en-US', options);
  }

  function updateInputField() {
    var queryType = $("#input-queryType").val();

    if (queryType === "leadId") {
      $("#tags").attr("placeholder", "Search by Lead Id");
      $("#tags").attr("type", "number");
    } else if (queryType === "contactName" || queryType === "phoneNumber") {
      $("#tags").attr("placeholder", "Search by " + queryType);
      $("#tags").attr("type", "text");
    }
  }
</script>

